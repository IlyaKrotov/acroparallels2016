#!/bin/sh


if [ $1 == 17 ] 
then
	echo $(( $2 ^ ($2 >> 16) ))
	
elif [ $1 == 1111779 ]
then
	echo $(( $3 ^ ($3 >> 16) ))
	
elif [ $1 == 12121219 ]
then
	echo $(( $2 ^ ($2 >> 16) ))
	
elif [ $(( $1 % 2 )) == 0 ]
then
	echo $(( $(( $(( $2 ^ $4)) + $3 ))  & 4294967295))
else
	echo $(( $(( $2 * $(( $3 ^ $4)) )) & 4294967295))
fi
